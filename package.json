{
  "name": "sb-tech-talk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.9.0",
    "@testing-library/react": "10.2.0",
    "@testing-library/user-event": "11.1.0",
    "clsx": "1.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "build-storybook": "build-storybook -s public",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src --cache",
    "posttest": "yarn lint",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test:cypress:ci": "cypress run",
    "test:cypress": "cypress open",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "cypress/globals": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env CI=true yarn test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/helper-call-delegate": "7.10.1",
    "@storybook/addon-actions": "6.0.0-beta.16",
    "@storybook/addon-controls": "6.0.0-beta.16",
    "@storybook/addon-docs": "6.0.0-beta.16",
    "@storybook/addon-links": "6.0.0-beta.16",
    "@storybook/addons": "6.0.0-beta.16",
    "@storybook/preset-create-react-app": "3.0.0",
    "@storybook/react": "6.0.0-beta.16",
    "@types/jest": "25.2.3",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "cypress": "4.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "prettier": "2.0.5",
    "typescript": "3.9.5"
  }
}
